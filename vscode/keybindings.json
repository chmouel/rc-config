[
    {
        "key": "alt+b",
        "command": "cursorWordEndLeft",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+f",
        "command": "cursorWordEndRight",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+d",
        "command": "deleteWordRight",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+k ctrl+e",
        "command": "deleteAllRight",
        "when": "editorTextFocus"
    },
    {
        "key": "cmd+k cmd+k",
        "command": "workbench.action.focusNextGroup",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+k t",
        "command": "go.toggle.test.file",
        "when": "editorLangId == go"
    },
    {
        "key": "ctrl+k r",
        "command": "go.test.cursor",
        "when": "editorLangId == go && editorTextFocus && resourceFilename =~ /_test.go/"
    },
    {
        "key": "ctrl+k shift+r",
        "command": "go.debug.cursor",
        "when": "editorLangId == go && editorTextFocus && resourceFilename =~ /_test.go/"
    },
    {
        "key": "ctrl+k p",
        "command": "editor.action.insertSnippet",
        "when": "editorLangId == go && editorTextFocus",
        "args": {
            "langId": "go",
            "name": "Print"
        }
    },
    {
        "key": "ctrl+k p",
        "when": "editorLangId == rust && editorTextFocus",
        "command": "editor.action.insertSnippet",
        "args": {
            "langId": "rust",
            "name": "Print"
        }
    },
    {
        "key": "ctrl+k shift+p",
        "command": "editor.action.insertSnippet",
        "when": "editorLangId == go && editorTextFocus",
        "args": {
            "langId": "go",
            "name": "Spew Dump"
        }
    },
    {
        "key": "ctrl+k v",
        "command": "git.openChange",
        "when": "editorTextFocus"
    },
    {
        "key": "f3",
        "command": "go.test.previous",
        "when": "editorLangId == go && editorTextFocus"
    },
    {
        "key": "ctrl+f3",
        "command": "go.test.cursor",
        "when": "editorLangId == go && editorTextFocus"
    },
    {
        "key": "shift+f3",
        "command": "go.debug.cursor",
        "when": "editorLangId == go && editorTextFocus && resourceFilename =~ /_test.go/"
    },
    {
        "key": "f12",
        "command": "editor.action.goToDeclaration",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+tab",
        "command": "workbench.action.nextEditor"
    },
    {
        "key": "ctrl+shift+tab",
        "command": "workbench.action.previousEditor"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.quickOpenLeastRecentlyUsedEditor",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+e",
        "command": "cursorEnd",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+k ctrl+a",
        "command": "editor.action.selectAll",
        "when": "editorTextFocus"
    },
    {
        "key": "meta+w",
        "command": "workbench.action.closeActiveEditor",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+down",
        "command": "editor.action.copyLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+cmd+space",
        "command": "editor.action.triggerParameterHints",
        "when": "editorHasSignatureHelpProvider && editorTextFocus"
    },
    {
        "key": "ctrl+k shift+1",
        "command": "workbench.action.problems.focus",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+right",
        "command": "cursorWordEndRight",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+left",
        "command": "cursorWordEndLeft",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+right",
        "command": "cursorWordStartRightSelect",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+left",
        "command": "cursorWordStartLeftSelect",
        "when": "editorTextFocus"
    },
    {
        "key": "cmd+1",
        "command": "workbench.action.openEditorAtIndex1",
        "when": "editorTextFocus"
    },
    {
        "key": "cmd+2",
        "command": "workbench.action.openEditorAtIndex2",
        "when": "editorTextFocus"
    },
    {
        "key": "cmd+3",
        "command": "workbench.action.openEditorAtIndex2",
        "when": "editorTextFocus"
    },
    {
        "key": "cmd+4",
        "command": "workbench.action.openEditorAtIndex4",
        "when": "editorTextFocus"
    },
    {
        "key": "cmd+5",
        "command": "workbench.action.openEditorAtIndex5",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+w",
        "command": "workbench.action.closeActiveEditor"
    },
    {
        "key": "ctrl+w",
        "command": "-workbench.action.closeActiveEditor"
    },
    {
        "key": "ctrl+\\",
        "command": "workbench.action.showAllEditorsByMostRecentlyUsed"
    },
    {
        "key": "ctrl+shift+f3",
        "command": "go.debug.previous"
    },
    {
        "key": "ctrl+k shift+w",
        "command": "workbench.action.closeOtherEditors"
    },
    {
        "key": "ctrl+a",
        "command": "cursorHome",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+w",
        "command": "-expand_region",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+w",
        "command": "-workbench.action.closeWindow"
    },
    {
        "key": "cmd+k l",
        "command": "testing.reRunLastRun"
    },
    {
        "key": "cmd+; l",
        "command": "-testing.reRunLastRun"
    }
]