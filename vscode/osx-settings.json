{
    "diffEditor.ignoreTrimWhitespace": true,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.cursorSurroundingLinesStyle": "all",
    "workbench.tree.indent": 20,
    "editor.dragAndDrop": false,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.lineNumbers": "on",
    "editor.minimap.enabled": false,
    "editor.occurrencesHighlight": false,
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "trailing",
    "editor.scrollBeyondLastColumn": 0,
    "editor.scrollBeyondLastLine": false,
    "editor.selectionHighlight": false,
    "editor.smoothScrolling": false,
    "editor.suggest.localityBonus": true,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.tabCompletion": "on",
    "files.autoSave": "onFocusChange",
    "files.autoSaveDelay": 1000,
    "files.exclude": {
        "**/__pycache__": true,
        "**/.dir-locals.el": true,
        "**/*.pyc": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true,
        "**/vendor": true
    },
    "files.trimFinalNewlines": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "go.coverageOptions": "showUncoveredCodeOnly",
    "go.coverOnSingleTest": true,
    "go.formatTool": "gofumpt",
    "go.lintTool": "golangci-lint",
    "go.toolsManagement.autoUpdate": true,
    "go.useCodeSnippetsOnFunctionSuggest": true,
    "go.useLanguageServer": true,
    "go.testFlags": [
        "-v",
    ],
    "trailing-spaces.trimOnSave": true,
    "trailing-spaces.deleteModifiedLinesOnly": true,
    "[go]": {
        "editor.insertSpaces": false,
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "gopls": {
        "formatting.gofumpt": true,
    },
    "output.smartScroll.enabled": false,
    "python.formatting.provider": "yapf",
    "python.linting.mypyEnabled": true,
    "python.linting.pycodestyleEnabled": false,
    "python.linting.pylintEnabled": true,
    "settingsSync.ignoredSettings": [
        "workbench.colorTheme",
        "editor.fontSize",
        "editor.fontFamily",
        "editor.fontWeight",
        "terminal.integrated.fontFamily",
        "terminal.integrated.fontSize",
        "debug.console.fontSize",
        "debug.console.fontFamily"
    ],
    "terminal.integrated.allowChords": false,
    "terminal.integrated.cursorBlinking": false,
    "terminal.integrated.env.osx": {},
    "window.titleBarStyle": "native",
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.tips.enabled": false,
    "workbench.colorCustomizations": {
        "terminal.foreground": "#fff",
        "terminal.background": "#0000",
        "terminal.selectionBackground": "#595c5b",
    },
    "yaml.format.enable": true,
    "timeline.excludeSources": [
        "git-history"
    ],
    "editor.wrappingStrategy": "advanced",
    "redhat.telemetry.enabled": false,
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "yaml": true,
        "plaintext": true,
        "markdown": true,
        "jsonc": true,
        "rust": true
    },
    "[markdown]": {
        "editor.quickSuggestions": true
    },
    "html.autoClosingTags": false,
    "editor.bracketPairColorization.enabled": true,
    "python.analysis.completeFunctionParens": true,
    "telemetry.telemetryLevel": "off",
    "settingsSync.ignoredExtensions": [
        "github.vscode-pull-request-github",
        "panicbit.cargo",
        "matklad.rust-analyzer",
        "vscodevim.vim",
        "pylint.path"
    ],
    "editor.fontSize": 16,
    "settingsSync.keybindingsPerPlatform": false,
    "editor.fontFamily": "JetBrains Mono",
    "zenMode.hideTabs": false,
    "explorer.confirmDelete": false,
    "editor.guides.bracketPairs": "active",
    "editor.guides.bracketPairsHorizontal": false,
    "terminal.integrated.fontSize": 16,
    "workbench.reduceMotion": "on",
    "editor.fontLigatures": true,
    "workbench.editor.enablePreview": false,
    "workbench.colorTheme": "GitHub Light",
    "workbench.editor.focusRecentEditorAfterClose": false,
    "files.associations": {
        "*.sh": "shellscript"
    },
    "editor.renderLineHighlight": "none",
    "vale.core.useCLI": true,
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "rust-analyzer.checkOnSave.command": "clippy",
    "debug.console.fontFamily": "Roboto Mono",
    "debug.console.fontSize": 20,
    "explorer.fileNesting.enabled": true,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "window.zoomLevel": 1,
    "vim.useSystemClipboard": true,
    "vim.hlsearch": true,
    "vim.leader": "<space>",
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>",
            ],
            "commands": [
                "workbench.action.files.save"
            ]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "`"
            ],
            "commands": [
                "workbench.action.terminal.toggleTerminal"
            ]
        },
        {
            "before": [
                "<C-e>"
            ],
            "commands": [
                "workbench.action.quickOpenPreviousRecentlyUsedEditor"
            ]
        },
        {
            "before": [
                "<Leader>",
                "<C-r>"
            ],
            "commands": [
                "workbench.action.quickOpenRecent"
            ]
        },
        {
            "before": [
                "<C-f>"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "<S-Tab>"
            ],
            "commands": [
                "workbench.action.previousEditor"
            ],
            "silent": true
        },
        {
            "before": [
                "<Tab>"
            ],
            "commands": [
                "workbench.action.nextEditor"
            ],
            "silent": true
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "leader",
                "w"
            ],
            "commands": [
                "workbench.action.files.save"
            ]
        },
        {
            "before": [
                "<Leader>",
                "g"
            ],
            "after": [
                "<leader>",
                "<leader>",
                "s"
            ]
        },
        {
            "before": [
                "u"
            ],
            "after": [],
            "commands": [
                {
                    "command": "undo"
                }
            ]
        },
        {
            "before": [
                "<C-r>"
            ],
            "after": [],
            "commands": [
                {
                    "command": "redo"
                }
            ]
        },
    ],
    "vim.cursorStylePerMode.insert": "underline",
    "vim.easymotion": true,
    "vim.camelCaseMotion.enable": true,
    "vim.sneak": true,
    "vim.smartRelativeLine": true,
    "security.workspace.trust.untrustedFiles": "open",
    "rust-analyzer.completion.privateEditable.enable": true,
    "rust-analyzer.rustfmt.rangeFormatting.enable": true,
}